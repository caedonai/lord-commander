{
  "name": "lord-commander-monorepo",
  "private": true,
  "version": "3.0.0",
  "description": "Modern CLI SDK Monorepo with UI, API, and CLI applications",
  "type": "module",
  "workspaces": [
    "libs/*",
    "apps/*"
  ],
  "scripts": {
    "build": "pnpx nx affected --target=build --parallel=3",
    "build:all": "pnpx nx run-many --target=build --parallel=3",
    "lint": "pnpx nx affected --target=lint --parallel=3",
    "lint:fix": "pnpx nx affected --target=lint:fix --parallel=3",
    "lint:fix:all": "pnpx nx run-many --target=lint:fix --parallel=3",
    "lint:check": "pnpx nx affected --target=lint --parallel=3",
    "lint:check:all": "pnpx nx run-many --target=lint --parallel=3",
    "test": "pnpx nx affected --target=test --parallel=2",
    "test:watch": "pnpx nx test cli-core --watch",
    "test:coverage": "NODE_OPTIONS=\"--max-old-space-size=4096\" pnpx nx test cli-core --coverage",
    "test:coverage:text": "pnpx nx test cli-core --coverage --reporter=text",
    "test:ui": "pnpx nx test cli-core --ui",
    "test:all": "pnpx nx run-many --target=test --parallel=2",
    "ci": "pnpm lint:check:all && pnpm build:all && pnpm test:all",
    "pre-commit": "pnpm lint && pnpm test",
    "pre-commit:fix": "pnpm lint:fix && pnpm test",
    "hooks:install": "git config core.hooksPath .githooks && chmod +x .githooks/*",
    "hooks:test": "./.githooks/pre-commit",
    "setup": "node libs/cli-core/scripts/setup-dev.mjs",
    "release": "tsx libs/cli-core/scripts/release.ts",
    "analyze": "tsx libs/cli-core/scripts/generate-bundle-docs.ts",
    "docs:generate": "tsx libs/cli-core/scripts/generate-api-docs.ts",
    "docs:fix-links": "tsx libs/cli-core/scripts/fix-api-links.ts",
    "docs:update-metrics": "tsx libs/cli-core/scripts/update-readme-metrics.ts",
    "docs:bundle-analysis": "tsx libs/cli-core/scripts/generate-bundle-docs.ts",
    "docs:performance": "tsx libs/cli-core/scripts/generate-performance-docs.ts",
    "docs:update-all": "tsx libs/cli-core/scripts/update-all-docs.ts",
    "dev:setup": "node libs/cli-core/scripts/setup-dev.mjs",
    "bundle:analyze": "tsx libs/cli-core/scripts/generate-bundle-docs.ts"
  },
  "keywords": [
    "cli",
    "sdk",
    "framework",
    "command-line",
    "tool",
    "typescript",
    "commander",
    "scaffolding",
    "automation"
  ],
  "author": "fabiomarcellusdev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/caedonai/lord-commander-poc.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.1",
    "execa": "^8.0.1",
    "figures": "^6.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.553.0",
    "next": "~15.2.4",
    "picocolors": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.3.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8",
    "@sanity/client": "^7.8.0",
    "@sanity/image-url": "^1.1.0",
    "@portabletext/react": "^3.1.0",
    "@sanity/vision": "^3.62.2",
    "next-sanity": "^9.10.4",
    "sanity": "^3.62.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.5",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/cypress": "22.0.2",
    "@nx/esbuild": "^22.0.2",
    "@nx/js": "22.0.2",
    "@nx/nest": "22.0.2",
    "@nx/next": "^22.0.2",
    "@nx/node": "^22.0.2",
    "@nx/vite": "22.0.2",
    "@nx/web": "22.0.2",
    "@nx/webpack": "22.0.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.15.1",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.0.0",
    "cypress": "^14.2.1",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "22.0.2",
    "prettier": "^2.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "ultracite": "^6.1.0",
    "unplugin-swc": "^1.5.8",
    "vite": "^7.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.2.4",
    "webpack-cli": "^5.1.4"
  },
  "nx": {}
}
