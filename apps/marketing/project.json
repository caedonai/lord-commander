{
  "name": "marketing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/marketing/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "config": "apps/marketing/vitest.config.ts",
        "passWithNoTests": false,
        "reportsDirectory": "coverage/apps/marketing"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ultracite check apps/marketing/src"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ultracite fix apps/marketing/src"
      }
    }
  }
}
